<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="page.main.title">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Mageinn_Seo::html/title.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="product.info.overview">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Mageinn_Seo::product/view/attribute.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>